<?xml version="1.0"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "mathml.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">

<body>

<math display="block" xmlns="http://www.w3c.org/1998/Math/MathML">
	<mrow>
		<mo>(</mo>
		<mfrac>
			<mrow>
				<mo>(</mo>
				<mi mathsize="big">X</mi>
					<mfrac>
						<mi>top</mi>
						<mi>bottom</mi>
					</mfrac>
				<mo>)</mo>
				<mo>(</mo>
				<mi mathsize="big">X</mi>
					<mfrac>
						<mrow>
							<mo>(</mo>
							<mi mathsize="big">X</mi>
							<mfrac>
								<mrow>
									<mo>(</mo>
									<mi mathsize="big">X</mi>
									<mfrac>
										<mrow>
											<mo>(</mo>
											<mi mathsize="big">X</mi>
											<mfrac>
												<mi>top</mi>
												<mrow>
													<mo>(</mo>
													<mi mathsize="big">X</mi>
													<mfrac>
														<mi>top</mi>
														<mi>bottom</mi>
													</mfrac>
													<mo>)</mo>
												</mrow>
											</mfrac>
											<mo>)</mo>
										</mrow>
										<mi>bottom</mi>
									</mfrac>
									<mo>)</mo>
								</mrow>
								<mi>bottom</mi>
							</mfrac>
							<mo>)</mo>
						</mrow>
						<mi>bottom</mi>
					</mfrac>
				<mo>)</mo>				
			</mrow>
			<mrow>
				<mfrac>
					<mi mathsize="big">Numerator</mi>
					<mi mathsize="big">Denominator And Some More Text and Some More Text</mi>
				</mfrac>
			</mrow>
		</mfrac>
		<mo>)</mo>
	</mrow>
</math>

</body>
</html>