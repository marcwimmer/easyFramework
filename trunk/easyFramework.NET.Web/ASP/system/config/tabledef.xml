<?xml version="1.0" encoding="utf-8" ?>
<table name="tsConfig" inserted="cfg_inserted" updated="cfg_updated" insertedby="cfg_insertedby"
	updatedby="cfg_updatedby">
	<field primarykey="1">
		<name>cfg_id</name>
		<type>int</type>
		<desc>Konfig-ID</desc>
	</field>
	<field>
		<name>cfg_name</name>
		<type>string</type>
		<size>128</size>
		<desc>Konfig-Name</desc>
	</field>
	<field>
		<name>cfg_value</name>
		<type>string</type>
		<desc>Konfigurationswert</desc>
		<size>2048</size>
	</field>
	<field>
		<name>cfg_inserted</name>
		<type>date</type>
		<desc>Eingefügt</desc>
	</field>
	<field>
		<name>cfg_updated</name>
		<type>date</type>
		<desc>Bearbeitet</desc>
	</field>
	<field>
		<name>cfg_insertedBy</name>
		<type>string</type>
		<desc>Eingefügt von</desc>
		<size>255</size>
	</field>
	<field>
		<name>cfg_updatedBy</name>
		<type>string</type>
		<desc>Bearbeitet von</desc>
		<size>255</size>
	</field>
</table>
